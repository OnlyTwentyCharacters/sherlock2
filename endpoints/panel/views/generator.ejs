<script>
$(() => {
    $("#gen").on("click", event => {
        $.post("/api/regkey", {}, data => {
            if (data.ok) {
                $("#key").text(data.key);
                $("#gen").prop("disabled", true);
            } else {
                $("#key").text("Error: " + data.error);
            }
        });

        return false;
    });
});
</script>

<code id="key">&nbsp;</code>

<input type="submit" value="Generate" id="gen">
